{
  "kind": "collectionType",
  "collectionName": "business_partner_payment_cards",
  "info": {
    "singularName": "business-partner-payment-card",
    "pluralName": "business-partner-payment-cards",
    "displayName": "Business Partner Payment Card"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "payment_card_id": {
